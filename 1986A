#include <bits/stdc++.h>
using namespace std;
using ll = long long;

ll gcd(ll a, ll b){if(b == 0) return a; return gcd(b, a % b);}
ll lcm(ll a, ll b){return a / gcd(a, b) * b;}
bool isPrime(ll n){if(n == 1) return 0; for(int i = 2; i <= round(sqrt(n)); i++) if(n % i == 0) return 0; return 1;}

int main(){
	ios_base::sync_with_stdio(0);
    cin.tie(0); cout.tie(0);
    
	int t; cin >> t;
	while(t--){
		int n = 3;
		vector<int> v(n);
		unordered_map<int, int> m;
		for(int i = 0; i < n; i++){
			cin >> v[i];
			m[v[i]]++;
		}
		int check = 0;
		for(const auto& pair : m){
			if(pair.second > 1){
				check = 1;
			}
		}
		sort(v.begin(), v.end());
		if(check){
			cout << v[2] - v[0] << endl;
		}
		else{
			cout << (v[2] - v[1]) + (v[1] - v[0]) << endl;
		}
	}
	
	return 0;
}
